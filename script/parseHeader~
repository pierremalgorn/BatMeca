#!/bin/bash

touch header
ligne=`sed -n -e '/Acquisition{0,1}/=' $1`;
echo ligne = $ligne;
let fin=$ligne+2;
echo fin = $fin;

#echo $cmd;
sed -n -e "/Acquisition/,$fin p" $1 >> header;
sed -n -e "/Acquisition/,$fin p" $1;
#sed -i -e "/Acquisition/d" header

#sed -n -e '/Acquisition/, 2 p' $1;
#sed -n 1p $1 >> header
#sed -n 2p $1 >> header
#sed -n 3p $1 >> header

#sed -i -e "s/,/./g" header

sed -i -e "s/#//g" header
sed -i -e "s/\t\t\t\t\t\t\t\t\t/,/g" header
sed -i -e "s/\t\t\t\t\t\t/,/g" header
sed -i -e "s/\t\t\t\t\t/,/g" header
sed -i -e "s/\t/,/g" header
